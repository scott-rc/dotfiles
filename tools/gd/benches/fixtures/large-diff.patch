-export function log(msg: string) {
-  console.log(msg);
+type LogLevel = 'debug' | 'info' | 'warn' | 'error';
+
+const LEVELS: Record<LogLevel, number> = { debug: 0, info: 1, warn: 2, error: 3 };
+const currentLevel: LogLevel = (process.env.LOG_LEVEL as LogLevel) || 'info';
+
+function shouldLog(level: LogLevel): boolean {
+  return LEVELS[level] >= LEVELS[currentLevel];
+}
+
+export const logger = {
+  debug: (msg: string, ...args: unknown[]) => shouldLog('debug') && console.debug(msg, ...args),
+  info: (msg: string, ...args: unknown[]) => shouldLog('info') && console.info(msg, ...args),
+  warn: (msg: string, ...args: unknown[]) => shouldLog('warn') && console.warn(msg, ...args),
+  error: (msg: string, ...args: unknown[]) => shouldLog('error') && console.error(msg, ...args),
 };
