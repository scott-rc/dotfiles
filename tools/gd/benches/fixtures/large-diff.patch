-export class Queue {
-  // TODO: implement job queue
+import { EventEmitter } from './events';
+import { logger } from './logger';
+
+interface Job<T = unknown> {
+  id: string;
+  type: string;
+  payload: T;
+  attempts: number;
+  maxAttempts: number;
+  createdAt: Date;
+}
+
+export class Queue extends EventEmitter {
+  private jobs: Job[] = [];
+  private processing = false;
+
+  async enqueue<T>(type: string, payload: T, maxAttempts = 3): Promise<string> {
+    const id = `${type}-${Date.now()}-${Math.random().toString(36).slice(2)}`;
+    this.jobs.push({ id, type, payload, attempts: 0, maxAttempts, createdAt: new Date() });
+    logger.info(`Enqueued job ${id} of type ${type}`);
+    if (!this.processing) this.process();
+    return id;
+  }
+
+  private async process(): Promise<void> {
+    this.processing = true;
+    while (this.jobs.length > 0) {
+      const job = this.jobs.shift()!;
+      job.attempts++;
+      try {
+        this.emit('process', job);
+        logger.info(`Processed job ${job.id}`);
+      } catch (err) {
+        logger.error(`Job ${job.id} failed (attempt ${job.attempts}/${job.maxAttempts})`);
+        if (job.attempts < job.maxAttempts) {
+          this.jobs.push(job);
+        } else {
+          this.emit('failed', job);
+        }
+      }
+    }
+    this.processing = false;
+  }
 }
