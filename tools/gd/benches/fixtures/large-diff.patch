+import { Pool, PoolConfig } from 'pg';
+import { dbConfig } from '@/config/database';
+import { logger } from './logger';
+
+let pool: Pool | null = null;
+
+function getPool(): Pool {
+  if (!pool) {
+    const config: PoolConfig = {
+      host: dbConfig.host,
+      port: dbConfig.port,
+      database: dbConfig.database,
+      user: dbConfig.user,
+      password: dbConfig.password,
+      min: dbConfig.pool.min,
+      max: dbConfig.pool.max,
+      idleTimeoutMillis: dbConfig.pool.idleTimeoutMs,
+    };
+    pool = new Pool(config);
+    pool.on('error', (err) => logger.error('Pool error', err));
+  }
+  return pool;
+}
+
+export const db = {
+  async query(sql: string, params?: unknown[]) {
+    const start = Date.now();
+    const result = await getPool().query(sql, params);
+    logger.debug(`Query ${sql} took ${Date.now() - start}ms`);
+    return result.rows[0] ?? null;
+  },
+  async count(table: string, where?: Record<string, unknown>) {
+    const clause = where ? Object.keys(where).map((k) => `${k} = ?`).join(' AND ') : '1=1';
+    const result = await getPool().query(`SELECT COUNT(*) as count FROM ${table} WHERE ${clause}`);
+    return parseInt(result.rows[0].count, 10);
+  },
+  async sum(table: string, column: string, where?: Record<string, unknown>) {
+    const clause = where ? Object.keys(where).map((k) => `${k} = ?`).join(' AND ') : '1=1';
+    const result = await getPool().query(`SELECT SUM(${column}) as total FROM ${table} WHERE ${clause}`);
+    return parseFloat(result.rows[0].total) || 0;
+  },
+  async insert(table: string, data: Record<string, unknown>) {
+    const keys = Object.keys(data);
+    const values = Object.values(data);
+    const placeholders = keys.map((_, i) => `$${i + 1}`).join(', ');
+    const result = await getPool().query(
+      `INSERT INTO ${table} (${keys.join(', ')}) VALUES (${placeholders}) RETURNING *`,
+      values,
+    );
+    return result.rows[0];
+  },
 };
