-export async function retry(fn: () => Promise<any>, attempts: number) {
-  return fn();
+import { logger } from './logger';
+
+interface RetryOptions {
+  attempts: number;
+  delayMs: number;
+  backoff: 'linear' | 'exponential';
+}
+
+const defaults: RetryOptions = { attempts: 3, delayMs: 1000, backoff: 'exponential' };
+
+export async function retry<T>(fn: () => Promise<T>, opts: Partial<RetryOptions> = {}): Promise<T> {
+  const { attempts, delayMs, backoff } = { ...defaults, ...opts };
+  let lastError: Error | undefined;
+
+  for (let i = 0; i < attempts; i++) {
+    try {
+      return await fn();
+    } catch (err) {
+      lastError = err as Error;
+      const delay = backoff === 'exponential' ? delayMs * 2 ** i : delayMs * (i + 1);
+      logger.warn(`Retry ${i + 1}/${attempts} failed, waiting ${delay}ms`);
+      await new Promise((resolve) => setTimeout(resolve, delay));
+    }
+  }
+
+  throw lastError;
 }
