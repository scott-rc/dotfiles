{
  "name": "md",
  "version": "0.1.0",
  "exports": "./mod.ts",
  "tasks": {
    "dev": "deno run -A main.ts",
    "test": "deno test -A",
    "install": "deno install --global -A --config deno.json --name md -f main.ts",
    "generate-fixtures": "deno run -A generate_fixtures.ts",
    "verify-fixtures": "deno run -A verify_fixtures.ts",
    "compat": "sh compat/run_compat_tests.sh 'deno run -A main.ts' && sh compat/cli_tests.sh 'deno run -A main.ts'"
  },
  "imports": {
    "@std/cli": "jsr:@std/cli@^1",
    "@std/fmt": "jsr:@std/fmt@^1",
    "@std/assert": "jsr:@std/assert@^1",
    "@std/front-matter": "jsr:@std/front-matter@^1",
    "marked": "npm:marked@^15",
    "shiki": "npm:shiki@^3"
  }
}
